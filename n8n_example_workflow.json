    {
      "parameters": {
        "jsCode": "// Procesar los datos del scraper\nconst data = $input.all()[0].json;\n\n// Filtrar por cargo especÃ­fico\nconst ceos = data.filter(item => item.cargo === 'ceo');\nconst founders = data.filter(item => item.cargo === 'founder');\n\n// Agregar timestamp\nconst processedData = data.map(item => ({\n  ...item,\n  timestamp: new Date().toISOString(),\n  processed: true\n}));\n\nreturn {\n  json: {\n    total_results: data.length,\n    ceos_count: ceos.length,\n    founders_count: founders.count,\n    all_data: processedData\n  }\n};"
      },
